{
  "name": "omp-launcher",
  "private": true,
  "version": "1.6.3",
  "type": "module",
  "homepage": "https://github.com/openmultiplayer/launcher",
  "author": "Amyr (iAmir) Ahmady (https://github.com/AmyrAhmady)",
  "scripts": {
    "dev": "vite -- --ompdebug",
    "dev-ingame": "vite -- --ompdebug --ingame",
    "build": "tsc && vite build -- --omprelease",
    "build-ingame": "tsc && vite build -- --omprelease --ingame",
    "preview": "vite preview",
    "tauri": "tauri",
    "start": "tauri dev",
    "release": "tauri build -t i686-pc-windows-msvc",
    "installer": "cd src-tauri/nsis && makensis installer_manual.nsi"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@tauri-apps/api": "^1.6.0",
    "axios": "^1.13.4",
    "chart.js": "^4.5.1",
    "i18next": "^25.8.0",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "react": "^19.2.4",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "react-native-animatable": "^1.4.0",
    "react-native-big-list": "^1.6.4",
    "react-native-web": "^0.21.2",
    "tauri-plugin-upload-api": "https://github.com/tauri-apps/tauri-plugin-upload#v1",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.6.3",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/react-native": "^0.73.0",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-react-native-web": "^0.21.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
